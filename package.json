{
  "name": "h5webstorage",
  "version": "0.6.0",
  "description": "HTML5 WebStorage API for angular2",
  "scripts": {
    "compile": "tsc",
    "aot": "npm run ngc -p ./tsconfig-aot.json",
    "pretest": "npm run compile",
    "test": "karma start test/karma.conf.js",
    "test:c": "concurrently \"tsc --watch --sourceMap\" \"karma start test/karma.conf.js --no-single-run\"",
    "build": "tsc",
    "start": "concurrently \"tsc --watch\" \"npm run lite\"",
    "lite": "single-page-server -p=8080 -b=./ -f=./example/index.html"
  },
  "author": "SirDarquan <https://github.com/SirDarquan>",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "^0.19.26",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.1.0",
    "@angular/platform-server": "~2.1.0",
    "@types/es6-shim": "^0.31.2",
    "@types/jasmine": "^2.4.1",
    "codeclimate-test-reporter": "^0.3.1",
    "concurrently": "^2.0.0",
    "guid": "0.0.12",
    "jasmine-core": "^2.4.1",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-firefox-launcher": "~1.0.0",
    "karma-ie-launcher": "~1.0.0",
    "karma-jasmine": "~1.0.0",
    "karma-sauce-launcher": "~1.1.0",
    "karma-source-map-support": "^1.2.0",
    "karma-systemjs": "^0.13.0",
    "single-page-server": "^0.1.0",
    "typescript": "~2.0.0"
  },
  "peerDependencies": {}
}
